{
  "title": "Print Screen to Screenshot",
  "rules": [
    {
      "description": "Screenshot",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "print_screen",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "5",
              "modifiers": [
                "left_command",
                "left_shift"
              ]
            }
          ]
        }
      ]
    },
  ]
}
